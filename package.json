{
  "name": "mediashelf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*",
      "tooling/*"
    ],
    "catalog": {
      "@typescript/native-preview": "7.0.0-dev.20260107.1",
      "typescript": "^5.9.3"
    },
    "catalogs": {}
  },
  "packageManager": "bun@1.3.5",
  "scripts": {
    "bootstrap": "bun i",
    "clean": "git clean -xdf .turbo node_modules",
    "clean:workspaces": "turbo run clean",
    "format": "turbo run format --continue",
    "lint": "turbo run lint --continue",
    "check": "turbo run check --continue",
    "typecheck": "turbo run typecheck",
    "healthcheck": "turbo run healthcheck"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "1.3.5",
    "@typescript/native-preview": "catalog:",
    "turbo": "^2.7.2",
    "typescript": "catalog:"
  }
}
